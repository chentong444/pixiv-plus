      "128": "images/pixiv_plus128.png",
      "16": "images/pixiv_plus16.png",
      "32": "images/pixiv_plus32.png",
      "48": "images/pixiv_plus48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Pixiv Plus",
   "permissions": [ "contextMenus", "downloads", "webRequest", "storage", "webRequestBlocking", "*://www.pixiv.net/*", "*://i.pximg.net/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1",
   "web_accessible_resources": [ "images/*.png" ]
