{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "myjs.js" ],
      "matches": [ "*://www.pixiv.net/artworks/*", "*://www.pixivision.net/*", "*://www.pixiv.net/ranking.php?mode=daily*" ],
      "run_at": "document_end"
   } ],
